CREATE TABLE car (ID INTEGER AUTO_INCREMENT NOT NULL, DISTANCE INTEGER, REGISTRNUMBER VARCHAR(255), VINCOD VARCHAR(255), YEAR INTEGER, CLIENT_ID INTEGER, COLOROFCAR_ID INTEGER, MODELOFCAR_ID INTEGER, PRIMARY KEY (ID))
CREATE TABLE colorofcar (ID INTEGER AUTO_INCREMENT NOT NULL, NAMECOLOROFCAR VARCHAR(255), PRIMARY KEY (ID))
CREATE TABLE client (ID INTEGER AUTO_INCREMENT NOT NULL, EMAIL VARCHAR(255), FIRSTNAME VARCHAR(255), LASTNAME VARCHAR(255), MIDDLENAME VARCHAR(255), PHONE VARCHAR(255), PRIMARY KEY (ID))

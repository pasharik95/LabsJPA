CREATE TABLE process (ID INTEGER AUTO_INCREMENT NOT NULL, DATETIMEFINISH DATETIME, DATETIMESTART DATETIME, OPERATION_ID INTEGER, ORDER_ID INTEGER, WORKER_ID INTEGER, PRIMARY KEY (ID))
